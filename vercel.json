{
    "builds": [
      {
        "src": "vercel_wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "includeFiles": ["static/**", "templates/**", "media/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "vercel_wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "Fi.settings",
      "PYTHON_VERSION": "3.10",
      "DEBUG": "False",
      "ALLOWED_HOSTS": ".vercel.app,.now.sh,localhost",
      "SECRET_KEY": "django-insecure-j%-8-76virs+juw7$1f+r8)-o%pzmises=#cjx6as4&-=bz6hu",
      "DB_NAME": "postgres",
      "DB_USER": "postgres.nbpxztnttzefmrokqlbz",
      "DB_PASSWORD": "9562449137",
      "DB_HOST": "aws-0-ap-south-1.pooler.supabase.com",
      "DB_PORT": "5432"
    },
    "regions": ["iad1"],
    "public": true,
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
      {
        "source": "/static/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }
  